@keyframes home-rcm-bg-breathe
  0%
    filter blur(40px) brightness(0.8)
  100%
    filter blur(40px) brightness(1.2)
@keyframes item-header
  0%, 20%
    transform scale(1.1)
    opacity 0
@keyframes notice
  from
    width 100%
@keyframes notice-show
  from
    opacity 0
@keyframes home-rcm-button
  to
    left 200%
@keyframes dialog-shell
  0%
    background #0000
@keyframes dialog-open
  0%
    scale 0
@keyframes dialog-close
  0%
    scale 1
@keyframes home-list-show
  0%
    opacity 0
@keyframes item-loading
  0%
    background #8882
  100%
    background #8881